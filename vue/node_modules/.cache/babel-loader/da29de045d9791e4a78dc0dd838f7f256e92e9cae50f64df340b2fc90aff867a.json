{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const question = ref('');\n    const response = ref(null);\n    const loading = ref(false);\n    const askQuestion = async () => {\n      loading.value = true;\n      try {\n        const res = await axios.post('API_ENDPOINT', {\n          question: question.value\n        });\n        response.value = res.data.answer;\n      } catch (error) {\n        console.error(error);\n        // Здесь можете добавить обработку ошибок\n      } finally {\n        loading.value = false;\n      }\n    };\n    const handleKeyDown = event => {\n      if (event.key === 'Enter' && !event.shiftKey) {\n        event.preventDefault(); // Предотвратить перенос строки\n        askQuestion(); // Вызвать метод askQuestion() при нажатии Enter без Shift\n      }\n    };\n    return {\n      question,\n      response,\n      askQuestion,\n      loading,\n      handleKeyDown\n    };\n  }\n};","map":{"version":3,"names":["ref","axios","setup","question","response","loading","askQuestion","value","res","post","data","answer","error","console","handleKeyDown","event","key","shiftKey","preventDefault"],"sources":["/home/doduohor/vue/src/App.vue"],"sourcesContent":["<template>\n    <div class=\"chat-container\">\n        <textarea class=\"question-input\" v-model=\"question\" placeholder=\"Введите ваш вопрос здесь\"\n            @keydown=\"handleKeyDown\"></textarea>\n        <button class=\"send-button\" @click=\"askQuestion\">Отправить</button>\n        <div v-if=\"loading\" class=\"loading\">Загрузка...</div>\n        <div v-if=\"response\" class=\"response\">\n            Ответ: {{ response }}\n        </div>\n    </div>\n</template>\n  \n<script>\nimport { ref } from 'vue';\nimport axios from 'axios';\n\nexport default {\n    setup() {\n        const question = ref('');\n        const response = ref(null);\n        const loading = ref(false);\n\n        const askQuestion = async () => {\n            loading.value = true;\n            try {\n                const res = await axios.post('API_ENDPOINT', { question: question.value });\n                response.value = res.data.answer;\n            } catch (error) {\n                console.error(error);\n                // Здесь можете добавить обработку ошибок\n            } finally {\n                loading.value = false;\n            }\n        };\n\n        const handleKeyDown = (event) => {\n            if (event.key === 'Enter' && !event.shiftKey) {\n                event.preventDefault(); // Предотвратить перенос строки\n                askQuestion(); // Вызвать метод askQuestion() при нажатии Enter без Shift\n            }\n        };\n\n        return { question, response, askQuestion, loading, handleKeyDown };\n    }\n};\n</script>\n\n<style>\nbody {\n    background-color: #00000022;\n    /* Цвет фона страницы */\n    margin: 0;\n    padding: 0;\n}\n\n.chat-container {\n    max-width: 600px;\n    margin: 20px auto;\n    padding: 20px;\n    background-color: #1e1e1e;\n    border-radius: 15px;\n    box-shadow: 0 5px 8px rgba(0, 0, 0, 0.5);\n    color: #ffffff;\n}\n\n.question-input {\n    width: 95%;\n    padding: 15px;\n    margin-bottom: 15px;\n    background-color: #2a2a2a;\n    border: 1px solid #333333;\n    border-radius: 6px;\n    color: #ffffff;\n    resize: none;\n}\n\n.send-button {\n    padding: 10px 25px;\n    background-color: #4caf50;\n    color: white;\n    border: none;\n    border-radius: 6px;\n    cursor: pointer;\n    transition: background-color 0.3s, transform 0.2s;\n    display: block;\n    margin: 0 auto;\n}\n\n.send-button:hover {\n    background-color: #43a047;\n    transform: scale(1.05);\n}\n\n.loading {\n    text-align: center;\n    color: #4caf50;\n    margin-top: 10px;\n}\n\n.response {\n    margin-top: 20px;\n    padding: 15px;\n    background-color: #252525;\n    border: 1px solid #333333;\n    border-radius: 6px;\n    color: #ffffff;\n}\n</style>\n"],"mappings":"AAaA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,QAAO,GAAIH,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMI,QAAO,GAAIJ,GAAG,CAAC,IAAI,CAAC;IAC1B,MAAMK,OAAM,GAAIL,GAAG,CAAC,KAAK,CAAC;IAE1B,MAAMM,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC5BD,OAAO,CAACE,KAAI,GAAI,IAAI;MACpB,IAAI;QACA,MAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,cAAc,EAAE;UAAEN,QAAQ,EAAEA,QAAQ,CAACI;QAAM,CAAC,CAAC;QAC1EH,QAAQ,CAACG,KAAI,GAAIC,GAAG,CAACE,IAAI,CAACC,MAAM;MACpC,EAAE,OAAOC,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpB;MACJ,UAAU;QACNP,OAAO,CAACE,KAAI,GAAI,KAAK;MACzB;IACJ,CAAC;IAED,MAAMO,aAAY,GAAKC,KAAK,IAAK;MAC7B,IAAIA,KAAK,CAACC,GAAE,KAAM,OAAM,IAAK,CAACD,KAAK,CAACE,QAAQ,EAAE;QAC1CF,KAAK,CAACG,cAAc,CAAC,CAAC,EAAE;QACxBZ,WAAW,CAAC,CAAC,EAAE;MACnB;IACJ,CAAC;IAED,OAAO;MAAEH,QAAQ;MAAEC,QAAQ;MAAEE,WAAW;MAAED,OAAO;MAAES;IAAc,CAAC;EACtE;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}