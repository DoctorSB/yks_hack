{"ast":null,"code":"import { createCommentVNode as _createCommentVNode, renderList as _renderList, Fragment as _Fragment, openBlock as _openBlock, createElementBlock as _createElementBlock, toDisplayString as _toDisplayString, normalizeClass as _normalizeClass, vModelText as _vModelText, withModifiers as _withModifiers, withKeys as _withKeys, createElementVNode as _createElementVNode, withDirectives as _withDirectives } from \"vue\";\nconst _hoisted_1 = {\n  class: \"input-container\"\n};\nconst _hoisted_2 = [\"disabled\"];\nconst _hoisted_3 = [\"disabled\"];\nconst _hoisted_4 = {\n  key: 0\n};\nconst _hoisted_5 = {\n  key: 1\n};\nconst _hoisted_6 = [\"disabled\"];\nexport function render(_ctx, _cache, $props, $setup, $data, $options) {\n  return _openBlock(), _createElementBlock(_Fragment, null, [_createCommentVNode(\" –ë–ª–æ–∫ —á–∞—Ç–∞ \"), _createElementVNode(\"div\", {\n    class: _normalizeClass([\"chat-container\", {\n      'moved': $setup.messageSent\n    }])\n  }, [_createCommentVNode(\" –û—Ç–≤–µ—Ç –Ω–∞ –≤–æ–ø—Ä–æ—Å \"), (_openBlock(true), _createElementBlock(_Fragment, null, _renderList($setup.chatHistory, message => {\n    return _openBlock(), _createElementBlock(\"div\", {\n      key: message.text,\n      class: _normalizeClass([\"message\", {\n        'user-message': message.isUser,\n        'user-message-right': message.isUser\n      }])\n    }, _toDisplayString(message.text), 3 /* TEXT, CLASS */);\n  }), 128 /* KEYED_FRAGMENT */)), _createElementVNode(\"div\", _hoisted_1, [_createCommentVNode(\" –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞ \"), _withDirectives(_createElementVNode(\"textarea\", {\n    class: \"question-input\",\n    \"onUpdate:modelValue\": _cache[0] || (_cache[0] = $event => $setup.question = $event),\n    disabled: $setup.loading,\n    placeholder: \"–í–≤–µ–¥–∏—Ç–µ –≤–∞—à –≤–æ–ø—Ä–æ—Å –∑–¥–µ—Å—å\",\n    onKeydown: _cache[1] || (_cache[1] = _withKeys(_withModifiers((...args) => $setup.submitForm && $setup.submitForm(...args), [\"prevent\"]), [\"enter\"]))\n  }, null, 40 /* PROPS, HYDRATE_EVENTS */, _hoisted_2), [[_vModelText, $setup.question]]), _createCommentVNode(\" –ö–Ω–æ–ø–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ \"), _createElementVNode(\"button\", {\n    class: \"send-button\",\n    onClick: _cache[2] || (_cache[2] = (...args) => $setup.submitForm && $setup.submitForm(...args)),\n    disabled: $setup.loading\n  }, [_createCommentVNode(\" –í–º–µ—Å—Ç–æ —Ç–µ–∫—Å—Ç–∞ \\\"–ó–∞–≥—Ä—É–∑–∫–∞...\\\" –∏—Å–ø–æ–ª—å–∑—É–µ–º –∏–∫–æ–Ω–∫—É \"), $setup.loading ? (_openBlock(), _createElementBlock(\"span\", _hoisted_4, \"‚Ü∫\")) : (_openBlock(), _createElementBlock(\"span\", _hoisted_5, \"‚úâ\"))], 8 /* PROPS */, _hoisted_3), _createCommentVNode(\" –ö–Ω–æ–ø–∫–∞ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è –æ—Ç–≤–µ—Ç–∞ \"), _createElementVNode(\"button\", {\n    class: \"copy-button\",\n    onClick: _cache[3] || (_cache[3] = (...args) => $setup.copyResponse && $setup.copyResponse(...args)),\n    disabled: !$setup.response\n  }, \" üìÉ \", 8 /* PROPS */, _hoisted_6)])], 2 /* CLASS */)], 2112 /* STABLE_FRAGMENT, DEV_ROOT_FRAGMENT */);\n}","map":{"version":3,"names":["class","_createCommentVNode","_createElementVNode","_normalizeClass","$setup","messageSent","_createElementBlock","_Fragment","_renderList","chatHistory","message","key","text","isUser","_hoisted_1","question","$event","disabled","loading","placeholder","onKeydown","_cache","_withKeys","_withModifiers","args","submitForm","onClick","_hoisted_4","_hoisted_5","copyResponse","response","_hoisted_6"],"sources":["/home/doduohor/vue/src/App.vue"],"sourcesContent":["<template>\n    <!-- –ë–ª–æ–∫ —á–∞—Ç–∞ -->\n    <div class=\"chat-container\" :class=\"{ 'moved': messageSent }\">\n        <!-- –û—Ç–≤–µ—Ç –Ω–∞ –≤–æ–ø—Ä–æ—Å -->\n        <div v-for=\"message in chatHistory\" :key=\"message.text\" class=\"message\"\n            :class=\"{ 'user-message': message.isUser, 'user-message-right': message.isUser }\">\n            {{ message.text }}\n        </div>\n        <div class=\"input-container\">\n            <!-- –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞ -->\n            <textarea class=\"question-input\" v-model=\"question\" :disabled=\"loading\" placeholder=\"–í–≤–µ–¥–∏—Ç–µ –≤–∞—à –≤–æ–ø—Ä–æ—Å –∑–¥–µ—Å—å\"\n                @keydown.enter.prevent=\"submitForm\"></textarea>\n            <!-- –ö–Ω–æ–ø–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ -->\n            <button class=\"send-button\" @click=\"submitForm\" :disabled=\"loading\">\n                <!-- –í–º–µ—Å—Ç–æ —Ç–µ–∫—Å—Ç–∞ \"–ó–∞–≥—Ä—É–∑–∫–∞...\" –∏—Å–ø–æ–ª—å–∑—É–µ–º –∏–∫–æ–Ω–∫—É -->\n                <span v-if=\"loading\">&#8634;</span>\n                <span v-else>&#9993;</span>\n            </button>\n            <!-- –ö–Ω–æ–ø–∫–∞ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è –æ—Ç–≤–µ—Ç–∞ -->\n            <button class=\"copy-button\" @click=\"copyResponse\" :disabled=\"!response\">\n                &#128195\n            </button>\n        </div>\n    </div>\n</template>\n  \n<script>\nimport { ref } from 'vue';\n\nexport default {\n    setup() {\n        const question = ref(''); // –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞\n        const response = ref(null); // –û—Ç–≤–µ—Ç –Ω–∞ –≤–æ–ø—Ä–æ—Å\n        const loading = ref(false); // –ò–Ω–¥–∏–∫–∞—Ç–æ—Ä –∑–∞–≥—Ä—É–∑–∫–∏\n        const messageSent = ref(false); // –§–ª–∞–≥ –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤\n        const chatHistory = ref([]); // –ú–∞—Å—Å–∏–≤ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∏—Å—Ç–æ—Ä–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏–π\n\n        const submitForm = async () => {\n            if (question.value.trim() === '') return; // –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ –ø—É—Å—Ç—ã—Ö —Å–æ–æ–±—â–µ–Ω–∏–π\n            loading.value = true;\n            messageSent.value = true; // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ñ–ª–∞–≥ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏—è\n            // –ó–¥–µ—Å—å –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –∫–æ–¥ –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –∑–∞–ø—Ä–æ—Å–∞ –∫ API\n            // –î–ª—è –ø—Ä–∏–º–µ—Ä–∞, –ø—Ä–æ—Å—Ç–æ –∏–º–∏—Ç–∏—Ä—É–µ–º –ø–æ–ª—É—á–µ–Ω–∏–µ –æ—Ç–≤–µ—Ç–∞\n            setTimeout(() => {\n                response.value = question.value; // –£–¥–∞–ª–µ–Ω–∏–µ \"–û—Ç–≤–µ—Ç: \" –¥–ª—è –ø—Ä–æ—Å—Ç–æ—Ç—ã\n                loading.value = false;\n                chatHistory.value.push({ text: question.value, isUser: true }); // –î–æ–±–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\n                chatHistory.value.push({ text: response.value, isUser: false }); // –î–æ–±–∞–≤–∏—Ç—å –æ—Ç–≤–µ—Ç\n                question.value = ''; // –û—á–∏—Å—Ç–∏—Ç—å –ø–æ–ª–µ –≤–≤–æ–¥–∞ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏\n            }, 2000);\n        };\n\n        const copyResponse = () => {\n            if (response.value) {\n                // –°–æ–∑–¥–∞–µ–º –≤—Ä–µ–º–µ–Ω–Ω—ã–π —ç–ª–µ–º–µ–Ω—Ç –¥–ª—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è —Ç–µ–∫—Å—Ç–∞\n                const tempInput = document.createElement('textarea');\n                tempInput.value = response.value;\n                document.body.appendChild(tempInput);\n                tempInput.select();\n                document.execCommand('copy');\n                document.body.removeChild(tempInput);\n                // –ú–æ–∂–Ω–æ —Ç–∞–∫–∂–µ –¥–æ–±–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –æ–± —É—Å–ø–µ—à–Ω–æ–º –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–∏\n            }\n        };\n\n        return { question, response, submitForm, loading, messageSent, chatHistory, copyResponse };\n    },\n};\n</script>\n\n<style>\n/* –û–±—â–∏–µ —Å—Ç–∏–ª–∏ —Å—Ç—Ä–∞–Ω–∏—Ü—ã */\nbody {\n    background-color: #121212;\n    /* –¶–≤–µ—Ç —Ñ–æ–Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—ã */\n    margin: 0;\n    /* –£–±–∏—Ä–∞–µ–º –æ—Ç—Å—Ç—É–ø—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é */\n    font-family: Arial, sans-serif;\n    /* –®—Ä–∏—Ñ—Ç —Ç–µ–∫—Å—Ç–∞ */\n}\n\n.copy-button {\n    cursor: pointer;\n    margin-left: 5px;\n    background-color: #1e1e1e;\n    border: 1px solid #3a3a3a;\n    border-radius: 0 6px 6px 0;\n}\n\n/* –°—Ç–∏–ª–∏ –¥–ª—è –±–ª–æ–∫–∞ —á–∞—Ç–∞ */\n.chat-container {\n    position: absolute;\n    /* –ê–±—Å–æ–ª—é—Ç–Ω–æ–µ –ø–æ–∑–∏—Ü–∏–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ –±–ª–æ–∫–∞ */\n    top: 20px;\n    /* –û—Ç—Å—Ç—É–ø —Å–≤–µ—Ä—Ö—É */\n    left: 50%;\n    /* –ì–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω–æ–µ –≤—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –ø–æ —Ü–µ–Ω—Ç—Ä—É */\n    transform: translateX(-50%);\n    /* –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–æ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª–∏ */\n    width: 600px;\n    /* –®–∏—Ä–∏–Ω–∞ –±–ª–æ–∫–∞ —á–∞—Ç–∞ */\n    padding: 20px;\n    /* –í–Ω—É—Ç—Ä–µ–Ω–Ω–∏–µ –æ—Ç—Å—Ç—É–ø—ã */\n    background-color: #1e1e1e;\n    /* –¶–≤–µ—Ç —Ñ–æ–Ω–∞ */\n    border-radius: 10px;\n    /* –°–∫—Ä—É–≥–ª–µ–Ω–∏–µ —É–≥–ª–æ–≤ */\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);\n    /* –¢–µ–Ω—å –±–ª–æ–∫–∞ */\n    color: #ffffff;\n    /* –¶–≤–µ—Ç —Ç–µ–∫—Å—Ç–∞ */\n    transition: all 0.5s ease;\n    /* –ü–ª–∞–≤–Ω—ã–µ –∞–Ω–∏–º–∞—Ü–∏–∏ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å–≤–æ–π—Å—Ç–≤ */\n}\n\n@media screen and (max-width: 600px) {\n    .chat-container {\n        width: auto;\n        /* –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è —à–∏—Ä–∏–Ω–∞ –±–ª–æ–∫–∞ —á–∞—Ç–∞ –ø—Ä–∏ –º–∞–ª–µ–Ω—å–∫–∏—Ö —ç–∫—Ä–∞–Ω–∞—Ö */\n    }\n}\n\n.chat-container.moved {\n    top: auto;\n    /* –û—Ç–º–µ–Ω–∞ –≤–µ—Ä—Ö–Ω–µ–≥–æ –æ—Ç—Å—Ç—É–ø–∞ */\n    bottom: 20px;\n    /* –û—Ç—Å—Ç—É–ø —Å–Ω–∏–∑—É */\n    transform: translateX(-50%);\n    /* –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–æ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª–∏ */\n}\n\n/* –°—Ç–∏–ª–∏ –¥–ª—è —Å–æ–æ–±—â–µ–Ω–∏–π */\n.message {\n    margin-top: 10px;\n    border: 1px solid #ffffff;\n    /* –û—Ç—Å—Ç—É–ø —Å–≤–µ—Ä—Ö—É –º–µ–∂–¥—É —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏ */\n    margin-bottom: 10px;\n    /* –û—Ç—Å—Ç—É–ø —Å–Ω–∏–∑—É –º–µ–∂–¥—É —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏ */\n    padding: 6px;\n    /* –í–Ω—É—Ç—Ä–µ–Ω–Ω–∏–µ –æ—Ç—Å—Ç—É–ø—ã —Å–æ–æ–±—â–µ–Ω–∏–π */\n    border-radius: 6px;\n    /* –°–∫—Ä—É–≥–ª–µ–Ω–∏–µ —É–≥–ª–æ–≤ —Å–æ–æ–±—â–µ–Ω–∏–π */\n    max-width: 100%;\n    /* –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è —à–∏—Ä–∏–Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏–π */\n    word-wrap: break-word;\n    /* –ü–µ—Ä–µ–Ω–æ—Å –¥–ª–∏–Ω–Ω—ã—Ö —Å–ª–æ–≤ */\n}\n\n.user-message {\n    /* background-color: #4caf50; */\n    /* –¶–≤–µ—Ç —Ñ–æ–Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è */\n    border: 1px solid #11e738;\n    /* –ì—Ä–∞–Ω–∏—Ü–∞ —Å–æ–æ–±—â–µ–Ω–∏—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è */\n    color: white;\n    /* –¶–≤–µ—Ç —Ç–µ–∫—Å—Ç–∞ —Å–æ–æ–±—â–µ–Ω–∏—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è */\n    margin-top: 3px;\n    /* –û—Ç—Å—Ç—É–ø —Å–≤–µ—Ä—Ö—É –º–µ–∂–¥—É —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏ */\n    margin-bottom: 3px;\n    /* –û—Ç—Å—Ç—É–ø —Å–Ω–∏–∑—É –º–µ–∂–¥—É —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏ */\n    padding: 6px;\n    /* –í–Ω—É—Ç—Ä–µ–Ω–Ω–∏–µ –æ—Ç—Å—Ç—É–ø—ã —Å–æ–æ–±—â–µ–Ω–∏–π */\n    border-radius: 6px;\n    /* –°–∫—Ä—É–≥–ª–µ–Ω–∏–µ —É–≥–ª–æ–≤ —Å–æ–æ–±—â–µ–Ω–∏–π */\n    max-width: 100%;\n    /* –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è —à–∏—Ä–∏–Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏–π */\n    word-wrap: break-word;\n    /* –ü–µ—Ä–µ–Ω–æ—Å –¥–ª–∏–Ω–Ω—ã—Ö —Å–ª–æ–≤ */\n}\n\n/* –°—Ç–∏–ª–∏ –¥–ª—è –ø–æ–ª—è –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞ */\n.input-container {\n    display: flex;\n    /* –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ flexbox –¥–ª—è —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤ */\n    align-items: stretch;\n    /* –í—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –ø–æ –≤—ã—Å–æ—Ç–µ */\n    margin-bottom: 15px;\n    /* –û—Ç—Å—Ç—É–ø —Å–Ω–∏–∑—É –º–µ–∂–¥—É –ø–æ–ª–µ–º –≤–≤–æ–¥–∞ –∏ —Å–æ–æ–±—â–µ–Ω–∏–µ–º */\n}\n\n/* –°—Ç–∏–ª–∏ –¥–ª—è –ø–æ–ª—è –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞ */\n.question-input {\n    flex-grow: 1;\n    /* –ó–∞–Ω–∏–º–∞–µ—Ç –≤—Å–µ –¥–æ—Å—Ç—É–ø–Ω–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ */\n    padding: 12px;\n    /* –í–Ω—É—Ç—Ä–µ–Ω–Ω–∏–µ –æ—Ç—Å—Ç—É–ø—ã –ø–æ–ª—è –≤–≤–æ–¥–∞ */\n    background-color: #1e1e1e;\n    /* –¶–≤–µ—Ç —Ñ–æ–Ω–∞ –ø–æ–ª—è –≤–≤–æ–¥–∞ */\n    border: 1px solid #333333;\n    /* –ì—Ä–∞–Ω–∏—Ü–∞ –ø–æ–ª—è –≤–≤–æ–¥–∞ */\n    border-radius: 6px 0 0 6px;\n    /* –°–∫—Ä—É–≥–ª–µ–Ω–∏–µ —É–≥–ª–æ–≤ –ø–æ–ª—è –≤–≤–æ–¥–∞ */\n    color: #ffffff;\n    /* –¶–≤–µ—Ç —Ç–µ–∫—Å—Ç–∞ */\n    resize: none;\n    /* –ó–∞–ø—Ä–µ—Ç –Ω–∞ –∏–∑–º–µ–Ω–µ–Ω–∏–µ —Ä–∞–∑–º–µ—Ä–∞ –ø–æ–ª—è –≤–≤–æ–¥–∞ */\n}\n\n/* –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ —Å—Ç–∏–ª–∏ –¥–ª—è –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ –≤–≤–æ–¥–∞ */\n.question-input:disabled {\n    background-color: #3a3a3a;\n    /* –¢–µ–º–Ω–µ–µ —Ñ–æ–Ω –¥–ª—è –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ –ø–æ–ª—è */\n    cursor: not-allowed;