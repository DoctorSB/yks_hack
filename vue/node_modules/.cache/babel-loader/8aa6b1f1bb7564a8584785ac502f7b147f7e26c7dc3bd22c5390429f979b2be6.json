{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nexport default {\n  setup() {\n    const question = ref(''); // –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞\n    const response = ref(null); // –û—Ç–≤–µ—Ç –Ω–∞ –≤–æ–ø—Ä–æ—Å\n    const loading = ref(false); // –ò–Ω–¥–∏–∫–∞—Ç–æ—Ä –∑–∞–≥—Ä—É–∑–∫–∏\n    const messageSent = ref(false); // –§–ª–∞–≥ –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤\n    const chatHistory = ref([]); // –ú–∞—Å—Å–∏–≤ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∏—Å—Ç–æ—Ä–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏–π\n\n    const submitForm = async () => {\n      if (question.value.trim() === '') return; // –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ –ø—É—Å—Ç—ã—Ö —Å–æ–æ–±—â–µ–Ω–∏–π\n      loading.value = true;\n      messageSent.value = true; // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ñ–ª–∞–≥ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏—è\n      // –ó–¥–µ—Å—å –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –∫–æ–¥ –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –∑–∞–ø—Ä–æ—Å–∞ –∫ API\n      // –î–ª—è –ø—Ä–∏–º–µ—Ä–∞, –ø—Ä–æ—Å—Ç–æ –∏–º–∏—Ç–∏—Ä—É–µ–º –ø–æ–ª—É—á–µ–Ω–∏–µ –æ—Ç–≤–µ—Ç–∞\n      setTimeout(() => {\n        response.value = question.value; // –£–¥–∞–ª–µ–Ω–∏–µ \"–û—Ç–≤–µ—Ç: \" –¥–ª—è –ø—Ä–æ—Å—Ç–æ—Ç—ã\n        loading.value = false;\n        chatHistory.value.push({\n          text: question.value,\n          isUser: true\n        }); // –î–æ–±–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\n        chatHistory.value.push({\n          text: response.value,\n          isUser: false\n        }); // –î–æ–±–∞–≤–∏—Ç—å –æ—Ç–≤–µ—Ç\n        question.value = ''; // –û—á–∏—Å—Ç–∏—Ç—å –ø–æ–ª–µ –≤–≤–æ–¥–∞ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏\n      }, 2000);\n    };\n    return {\n      question,\n      response,\n      submitForm,\n      loading,\n      messageSent,\n      chatHistory\n    };\n  }\n};","map":{"version":3,"names":["ref","setup","question","response","loading","messageSent","chatHistory","submitForm","value","trim","setTimeout","push","text","isUser"],"sources":["/home/doduohor/vue/src/App.vue"],"sourcesContent":["<template>\n    <!-- –ë–ª–æ–∫ —á–∞—Ç–∞ -->\n    <div class=\"chat-container\" :class=\"{ 'moved': messageSent }\">\n        <!-- –û—Ç–≤–µ—Ç –Ω–∞ –≤–æ–ø—Ä–æ—Å -->\n        <div v-for=\"message in chatHistory\" :key=\"message.text\" class=\"message\"\n            :class=\"{ 'user-message': message.isUser, 'user-message-right': message.isUser }\">\n            {{ message.text }}\n        </div>\n        <div class=\"input-container\">\n            <!-- –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞ -->\n            <textarea class=\"question-input\" v-model=\"question\" placeholder=\"–í–≤–µ–¥–∏—Ç–µ –≤–∞—à –≤–æ–ø—Ä–æ—Å –∑–¥–µ—Å—å\"\n                @keydown.enter.prevent=\"submitForm\"></textarea>\n            <!-- –ö–Ω–æ–ø–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ -->\n            <button class=\"send-button\" @click=\"submitForm\" :disabled=\"loading\">\n                <!-- –í–º–µ—Å—Ç–æ —Ç–µ–∫—Å—Ç–∞ \"–ó–∞–≥—Ä—É–∑–∫–∞...\" –∏—Å–ø–æ–ª—å–∑—É–µ–º –∏–∫–æ–Ω–∫—É -->\n                <span v-if=\"loading\" class=\"loading-icon\">üîÑ</span>\n                <span v-else>&#9993;</span>\n            </button>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { ref } from 'vue';\n\nexport default {\n    setup() {\n        const question = ref(''); // –ü–æ–ª–µ –≤–≤–æ–¥–∞ —Ç–µ–∫—Å—Ç–∞\n        const response = ref(null); // –û—Ç–≤–µ—Ç –Ω–∞ –≤–æ–ø—Ä–æ—Å\n        const loading = ref(false); // –ò–Ω–¥–∏–∫–∞—Ç–æ—Ä –∑–∞–≥—Ä—É–∑–∫–∏\n        const messageSent = ref(false); // –§–ª–∞–≥ –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤\n        const chatHistory = ref([]); // –ú–∞—Å—Å–∏–≤ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∏—Å—Ç–æ—Ä–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏–π\n\n        const submitForm = async () => {\n            if (question.value.trim() === '') return; // –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ –ø—É—Å—Ç—ã—Ö —Å–æ–æ–±—â–µ–Ω–∏–π\n            loading.value = true;\n            messageSent.value = true; // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ñ–ª–∞–≥ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏—è\n            // –ó–¥–µ—Å—å –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –∫–æ–¥ –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –∑–∞–ø—Ä–æ—Å–∞ –∫ API\n            // –î–ª—è –ø—Ä–∏–º–µ—Ä–∞, –ø—Ä–æ—Å—Ç–æ –∏–º–∏—Ç–∏—Ä—É–µ–º –ø–æ–ª—É—á–µ–Ω–∏–µ –æ—Ç–≤–µ—Ç–∞\n            setTimeout(() => {\n                response.value = question.value; // –£–¥–∞–ª–µ–Ω–∏–µ \"–û—Ç–≤–µ—Ç: \" –¥–ª—è –ø—Ä–æ—Å—Ç–æ—Ç—ã\n                loading.value = false;\n                chatHistory.value.push({ text: question.value, isUser: true }); // –î–æ–±–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\n                chatHistory.value.push({ text: response.value, isUser: false }); // –î–æ–±–∞–≤–∏—Ç—å –æ—Ç–≤–µ—Ç\n                question.value = ''; // –û—á–∏—Å—Ç–∏—Ç—å –ø–æ–ª–µ –≤–≤–æ–¥–∞ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏\n            }, 2000);\n        };\n\n        return { question, response, submitForm, loading, messageSent, chatHistory };\n    }\n};\n</script>\n\n<style>\n/* –û–±—â–∏–µ —Å—Ç–∏–ª–∏ —Å—Ç—Ä–∞–Ω–∏—Ü—ã */\nbody {\n    background-color: #121212;\n    /* –¶–≤–µ—Ç —Ñ–æ–Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—ã */\n    margin: 0;\n    /* –£–±–∏—Ä–∞–µ–º –æ—Ç—Å—Ç—É–ø—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é */\n    font-family: Arial, sans-serif;\n    /* –®—Ä–∏—Ñ—Ç —Ç–µ–∫—Å—Ç–∞ */\n}\n\n/* –°—Ç–∏–ª–∏ –¥–ª—è –±–ª–æ–∫–∞ —á–∞—Ç–∞ */\n.chat-container {\n    position: absolute;\n    /* –ê–±—Å–æ–ª—é—Ç–Ω–æ–µ –ø–æ–∑–∏—Ü–∏–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ –±–ª–æ–∫–∞ */\n    top: 20px;\n    /* –û—Ç—Å—Ç—É–ø —Å–≤–µ—Ä—Ö—É */\n    left: 50%;\n    /* –ì–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω–æ–µ –≤—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –ø–æ —Ü–µ–Ω—Ç—Ä—É */\n    transform: translateX(-50%);\n    /* –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–æ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª–∏ */\n    width: 600px;\n    /* –®–∏—Ä–∏–Ω–∞ –±–ª–æ–∫–∞ —á–∞—Ç–∞ */\n    padding: 20px;\n    /* –í–Ω—É—Ç—Ä–µ–Ω–Ω–∏–µ –æ—Ç—Å—Ç—É–ø—ã */\n    background-color: #1e1e1e;\n    /* –¶–≤–µ—Ç —Ñ–æ–Ω–∞ */\n    border-radius: 10px;\n    /* –°–∫—Ä—É–≥–ª–µ–Ω–∏–µ —É–≥–ª–æ–≤ */\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);\n    /* –¢–µ–Ω—å –±–ª–æ–∫–∞ */\n    color: #ffffff;\n    /* –¶–≤–µ—Ç —Ç–µ–∫—Å—Ç–∞ */\n    transition: all